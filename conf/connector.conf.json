{
  "tokenDurationSec": 36000000,
  "statsPublishPeriodSec": 10,

  "retryDelaySec":  10,
  "retryJitterSec": 5,
  "retryBackoffDelaySec": 10,
  "retryMaxBackoffDelaySec": 60,

  "searches": [
    {
      "id": "jokes",
      "enabled": false,
      "path": "./tmp/jokes.json",
      "type": "FIND_AND_BIND",
      "conf": {}
    }
    , {
      "id": "cars",
      "enabled": true,
      "path": "./tmp/cars.json",
      "type": "FIND_AND_BIND",
      "conf": {}
    }
    , {
      "id": "bikes",
      "enabled": false,
      "path": "./tmp/bikes.json",
      "type": "FIND_AND_BIND",
      "conf": {}
    }
    , {
      "id": "portsmouth_twins",
      "enabled": false,
      "path": "./tmp/portsmouth_twins.json",
      "type": "FIND_AND_BIND",
      "conf": {}
    }
    , {
      "id": "rolling_stock",
      "enabled": false,
      "path": "./tmp/rolling_stock.json",
      "type": "FIND_AND_DESCRIBE",
      "conf": {
        "startDelaySec": 5,
        "pollFrequencySec": 5
      }
    }
  ],

  "twinMapper" : {
    "ontologyRoot": "https://smartrics.it/ont#",
    "locationPath": "$.region.location",
    "metadataPath": "$.region",
    "commentPrefix": "COVID data for: ",
    "labelPaths": ["$.region.province","$.region.name"],
    "idPaths": ["$.region.iso", "$.region.name", "$.region.province"],
    "feeds": [{
      "name": "daily_data",
      "path": "$"
    } ]
  }
}